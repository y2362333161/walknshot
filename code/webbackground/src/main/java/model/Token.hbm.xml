<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Token" table="token">
		<id name="id" type="int">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="uid" type="int">
			<column name="uid" />
		</property>
		<property name="passphrase" type="java.lang.String">
			<column name="passphrase" />
		</property>
	</class>
</hibernate-mapping>