<?xml version="1.0" encoding="utf-8"?>
<!-- Struts2 DTD -->
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<constant name="struts.i18n.encoding" value="utf-8" />
	<constant name="struts.devMode" value="true" />
	<package name="process" extends="struts-default">
		<!-- Comment -->
		<action name="allCommentsPro" class="action.AllCommentsAction">
			<result name="success"></result>
		</action>

		<!-- Path -->
		<action name="allPathsPro" class="action.AllPathsAction">
			<result name="success"></result>
		</action>

		<!-- LatLng -->
		<action name="allLatLngsPro" class="action.AllLatLngsAction">
			<result name="success"></result>
		</action>
		
		<!-- Photo -->
		<action name="allPhotosPro" class="action.AllPhotosAction">
			<result name="success"></result>
		</action>
		
		<!-- Manager -->
		<action name="allUsersPro" class="action.AllUsersAction">
			<result name="success">/walknshot/jsp/manager.jsp</result>
		</action>
		<action name="addUserPro" class="action.AddUserAction">
			<result name="success">/walknshot/jsp/manager.jsp</result>
		</action>
		<action name="updateUserPro" class="action.UpdateUserAction">
			<result name="success">/walknshot/jsp/manager.jsp</result>
		</action>
		<action name="deleteUserPro" class="action.DeleteUserAction">
			<result name="success">/walknshot/jsp/manager.jsp</result>
		</action>

		<action name="*">
			<result>/{1}.jsp</result>
		</action>
	</package>
</struts>